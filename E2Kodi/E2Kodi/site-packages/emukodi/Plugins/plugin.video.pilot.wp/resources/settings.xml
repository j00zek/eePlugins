<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<settings>
  <category label="Konto">
    <setting label="Login" type="text" id="username" default=""/>
    <setting label="Hasło" type="text" id="password" option="hidden" enable="!eq(-1,)" default=""/>
  </category>
  <category label="Kreator listy m3u">
    <setting label="Lista m3u (Nazwa Pliku oraz Katalog Docelowy)" type="lsep"/>
    <setting label="Nazwa" type="text" id="fname" default="userbouquet.WPPL.tv" />
    <setting label="Katalog" type="folder" id="path" source="auto" option="writeable" default="/etc/enigma2"/>
    <setting type="action"
             action="RunPlugin(plugin://plugin.video.pilot.wp?action=BUILD_M3U)"
             label="Utwórz listę m3u"
             option="close" />
  </category>
  <setting id="netviapisessid" value="" default=""/>  
  <setting id="netviapisessval" value="" default=""/>  
  <setting id="remote_cookies" value="" default=""/>  
  <setting id="mode" value="0" default="0"/>  
  <setting id="sessionid" value="" default=""/>  
  <setting id="path_m3u" value="" default="/tmp/.emukodi"/>  
  <setting id="m3u_filename" value="" default="pilotwp"/>  
</settings>
